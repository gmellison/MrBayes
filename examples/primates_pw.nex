#NEXUS

[ Data from: Hayasaka, K., T. Gojobori, and S. Horai. 1988. Molecular phylogeny and evolution of primate mitochondrial DNA. Mol. Biol. Evol. 5:626-644. ]

begin data;
    dimensions ntax=12 nchar=13;
    format datatype=dna interleave=no gap=-;
    matrix
Tarsius_syrichta    AAGTTTCATTGGA
Lemur_catta         AAGCTTCATAGGA
Homo_sapiens        AAGCTTCACCGGC
Pan                 AAGCTTCACCGGC
Gorilla             AAGCTTCACCGGC
Pongo               AAGCTTCACCGGC
Hylobates           AAGCTTTACAGGT
Macaca_fuscata      AAGCTTTTCCGGC
M_mulatta           AAGCTTTTCTGGC
M_fascicularis      AAGCTTCTCCGGC
M_sylvanus          AAGCTTCTCCGGT
Saimiri_sciureus    AAGCTTCACCGGC
    ;
end;

begin mrbayes;
    lset nst=1 nucmodel=4by4 pairwise=yes rates=gamma;
    lset pwweights=yes numdatasplits=3;
    prset shapepr=exponential(1);
    mcmc ngen=10000 samplefreq=1000 burnin=1000;
    sump;
    sumt;
end;
