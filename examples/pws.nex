#NEXUS

[ Data from: Hayasaka, K., T. Gojobori, and S. Horai. 1988. Molecular phylogeny and evolution of primate mitochondrial DNA. Mol. Biol. Evol. 5:626-644. ]

begin data;
    dimensions ntax=8 nchar=28;
    format datatype=dna interleave=no gap=-;
    matrix
Lemur_catta         AAAAAAAAAAGCTTCATAGGAGCAACCA
Homo_sapiens        AAAAAAAAAAGCTTCACCGGCGCAGTCA
Chimp               AAAAAAAAAAGCTTCACCGGCGCAATTA
Gorilla             AAAAAAAAAAGCTTCACCGGCGCAGTTG
Pongo               AAAAAAAAAAGCTTCACCGGCGCAACCA
Hylobates           AAAAAAAAAAGCTTTACAGGTGCAACCG
M_sylvanus          AAAAAAAAAAGCTTCTCCGGTGCAACTA
Saimiri_sciureus    AAAAAAAAAAGCTTCACCGGCGCAATGA
    ;
end;

begin mrbayes;
    lset nst=6 nucmodel=4by4  rates=gamma;
    lset pairwise=yes pwweights=yes;
    prset shapepr=exponential(1);
    mcmc ngen=10000 samplefreq=1000 burnin=1000;
    sump;
    sumt;
end;
